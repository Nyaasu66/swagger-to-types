{
  "name": "swagger-to-types",
  "displayName": "Swagger To Types",
  "description": "VSCode Extension: Transform swagger-api to typescript interface.",
  "version": "0.3.1",
  "icon": "assets/app-icon.png",
  "publisher": "lanten",
  "engines": {
    "vscode": "^1.41.0"
  },
  "main": "./out/src/main.js",
  "keywords": [
    "swagger",
    "typescript",
    "api",
    "interface"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:api.list",
    "onView:api.local"
  ],
  "contributes": {
    "commands": [
      {
        "command": "api.list.refresh",
        "title": "%command.refresh%",
        "icon": {
          "light": "assets/icons/refresh.light.svg",
          "dark": "assets/icons/refresh.dark.svg"
        }
      },
      {
        "command": "api.group.setting",
        "title": "%command.setting%",
        "icon": {
          "light": "assets/icons/setting.light.svg",
          "dark": "assets/icons/setting.dark.svg"
        }
      },
      {
        "command": "api.group.add",
        "title": "%command.add%",
        "icon": {
          "light": "assets/icons/add.light.svg",
          "dark": "assets/icons/add.dark.svg"
        }
      },
      {
        "command": "api.group.refresh",
        "title": "%command.refresh%",
        "icon": {
          "light": "assets/icons/refresh.light.svg",
          "dark": "assets/icons/refresh.dark.svg"
        }
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "swagger-to-types",
          "title": "%app.title%",
          "icon": "assets/icons/tree-view.dark.svg"
        }
      ]
    },
    "views": {
      "swagger-to-types": [
        {
          "id": "api.list",
          "name": "%view.list.title%"
        },
        {
          "id": "api.local",
          "name": "%view.local.title%"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "api.list.refresh",
          "group": "navigation@09",
          "when": "view == api.list"
        },
        {
          "command": "api.group.setting",
          "group": "navigation@12",
          "when": "view == api.local"
        },
        {
          "command": "api.group.add",
          "group": "navigation@11",
          "when": "view == api.local"
        },
        {
          "command": "api.group.refresh",
          "group": "navigation@19",
          "when": "view == api.local"
        }
      ]
    },
    "configuration": {
      "title": "swaggerToTypes",
      "properties": {
        "swaggerToTypes.swaggerJsonUrl": {
          "type": "array",
          "default": [],
          "description": "%text.config.swaggerJsonUrl%"
        },
        "swaggerToTypes.savePath": {
          "type": "string",
          "description": "%text.config.savePath%",
          "default": "types/swagger-to-types"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.2",
    "@types/node-fetch": "^2.5.7",
    "@types/vscode": "^1.49.0",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "node-fetch": "^2.6.1"
  },
  "author": {
    "name": "Lanten",
    "url": "https://www.lanten.me/",
    "email": "lanten233@qq.com"
  },
  "homepage": "https://github.com/lanten/swagger-to-types",
  "repository": {
    "type": "git",
    "url": "https://github.com/lanten/swagger-to-types.git"
  },
  "bugs": {
    "url": "https://github.com/lanten/swagger-to-types/issues",
    "email": "lanten233@qq.com"
  }
}
