{
  "name": "swagger-to-types",
  "displayName": "Swagger To Types",
  "description": "VSCode Extension: Transform swagger-api to typescript interface.",
  "version": "0.3.3",
  "icon": "assets/app-icon.png",
  "publisher": "lanten",
  "engines": {
    "vscode": "^1.41.0"
  },
  "main": "./out/src/main.js",
  "keywords": [
    "swagger",
    "typescript",
    "api",
    "interface"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "keybindings": [
      {
        "command": "cmd.list.search",
        "key": "alt+shift+f"
      }
    ],
    "commands": [
      {
        "command": "cmd.list.refresh",
        "title": "%command.list.refresh%",
        "category": "%app.category%",
        "icon": "$(refresh)"
      },
      {
        "command": "cmd.list.add",
        "title": "%command.add%",
        "category": "%app.category%",
        "icon": "$(add)"
      },
      {
        "command": "cmd.list.remove",
        "title": "%command.remove%",
        "icon": "$(trash)"
      },
      {
        "command": "cmd.list.search",
        "title": "%command.search%",
        "category": "%app.category%",
        "icon": "$(search)"
      },
      {
        "command": "cmd.list.saveInterface",
        "title": "%command.saveInterface%",
        "icon": "$(cloud-download)"
      },
      {
        "command": "cmd.local.refresh",
        "title": "%command.local.refresh%",
        "category": "%app.category%",
        "icon": "$(refresh)"
      },
      {
        "command": "cmd.common.setting",
        "title": "%command.setting%",
        "category": "%app.category%",
        "icon": "$(gear)"
      },
      {
        "command": "cmd.common.openLink",
        "title": "%command.openLink%",
        "icon": "$(link-external)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "swagger-to-types",
          "title": "%app.title%",
          "icon": "assets/icons/tree-view.dark.svg"
        }
      ]
    },
    "views": {
      "swagger-to-types": [
        {
          "id": "view.list",
          "name": "%view.list.title%"
        },
        {
          "id": "view.local",
          "name": "%view.local.title%"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "view.list",
        "contents": "%text.noWorkspace%",
        "when": "workbenchState == empty"
      },
      {
        "view": "view.list",
        "contents": "%viewsWelcome.emptyList%",
        "when": "workbenchState != empty"
      },
      {
        "view": "view.local",
        "contents": "%text.noWorkspace%",
        "when": "workbenchState == empty"
      },
      {
        "view": "view.local",
        "contents": "%viewsWelcome.emptyLocal%",
        "when": "workbenchState != empty"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "cmd.list.search",
          "group": "navigation@00",
          "when": "view == view.list"
        },
        {
          "command": "cmd.list.add",
          "group": "navigation@01",
          "when": "view == view.list"
        },
        {
          "command": "cmd.common.setting",
          "group": "navigation@02",
          "when": "view == view.list"
        },
        {
          "command": "cmd.list.refresh",
          "group": "navigation@09",
          "when": "view == view.list"
        },
        {
          "command": "cmd.common.setting",
          "group": "navigation@12",
          "when": "view == view.local"
        },
        {
          "command": "cmd.local.refresh",
          "group": "navigation@19",
          "when": "view == view.local"
        }
      ],
      "view/item/context": [
        {
          "command": "cmd.list.remove",
          "group": "inline@01",
          "when": "view == view.list && viewItem == root"
        },
        {
          "command": "cmd.common.openLink",
          "group": "inline@02",
          "when": "view == view.list && viewItem == root"
        },
        {
          "command": "cmd.list.saveInterface",
          "group": "inline@09",
          "when": "view == view.list && viewItem == interface || viewItem == group"
        }
      ]
    },
    "configuration": {
      "title": "swaggerToTypes",
      "properties": {
        "swaggerToTypes.swaggerJsonUrl": {
          "type": "array",
          "default": [],
          "description": "%text.config.swaggerJsonUrl%"
        },
        "swaggerToTypes.savePath": {
          "type": "string",
          "description": "%text.config.savePath%",
          "default": "types/swagger-to-types"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.2",
    "@types/node-fetch": "^2.5.7",
    "@types/vscode": "^1.41.0",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "node-fetch": "^2.6.1"
  },
  "author": {
    "name": "Lanten",
    "url": "https://www.lanten.me/",
    "email": "lanten233@qq.com"
  },
  "homepage": "https://github.com/lanten/swagger-to-types",
  "repository": {
    "type": "git",
    "url": "https://github.com/lanten/swagger-to-types.git"
  },
  "bugs": {
    "url": "https://github.com/lanten/swagger-to-types/issues",
    "email": "lanten233@qq.com"
  }
}
