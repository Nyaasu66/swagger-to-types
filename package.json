{
  "name": "swagger-to-types",
  "displayName": "swagger-to-types",
  "description": "获取 swagger json 转换为 typescript interface",
  "publisher": "lanten",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.38.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:api-list",
    "onView:api-group",
    "onCommand:extension.helloWorld"
  ],
  "main": "./dist/main.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.helloWorld",
        "title": "Hello World"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "app",
          "title": "%app.title%",
          "icon": "src/assets/icon-tree-view.svg"
        }
      ]
    },
    "views": {
      "app": [
        {
          "id": "api-list",
          "name": "%view.list.title%"
        },
        {
          "id": "api-group",
          "name": "%view.detail.title%"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "dev": "webpack --mode development --watch",
    "pretest": "yarn run compile"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.38.0",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-loader": "^3.0.1",
    "eslint-plugin-prettier": "^3.1.1",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "prettier": "^1.18.2",
    "standard-version": "^7.0.0",
    "ts-loader": "^6.1.2",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  }
}
