{
  "name": "swagger-to-types",
  "displayName": "swagger-to-types",
  "description": "获取 swagger json 转换为 typescript interface",
  "publisher": "lanten",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.38.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:api.list",
    "onCommand:extension.helloWorld"
  ],
  "main": "./out/main.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.helloWorld",
        "title": "Hello World"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "app",
          "title": "%app.title%",
          "icon": "src/assets/icon-tree-view.svg"
        }
      ]
    },
    "views": {
      "app": [
        {
          "id": "api.list",
          "name": "%view.list.title%"
        },
        {
          "id": "api.group",
          "name": "%view.detail.title%"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.38.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "typescript": "^3.3.1",
    "tslint": "^5.12.1",
    "vscode-test": "^1.2.0"
  }
}
